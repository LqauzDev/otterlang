# Generics examples

pub struct Pair<T, U>:
    first: T
    second: U

fn main():
    # Use Pair<T, U> with different concrete type arguments
    let int_str = Pair(first=1, second="one")
    let str_float = Pair(first="pi", second=3.14)

    print(int_str.first)
    print(int_str.second)
    print(str_float.first)
    print(str_float.second)
