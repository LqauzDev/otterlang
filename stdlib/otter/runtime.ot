def gos() -> int:
    return runtime.gos()

def cpu_count() -> int:
    return runtime.cpu_count()

def memory() -> int:
    return runtime.memory()

def collect_garbage():
    runtime.collect_garbage()

def stats() -> string:
    stats_json = runtime.stats()
    return stats_json

def version() -> string:
    version_str = runtime.version()
    return version_str

def free_string(ptr: string):
    runtime.free(ptr)
