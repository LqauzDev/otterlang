fn gos() -> int:
    return runtime.gos()

fn cpu_count() -> int:
    return runtime.cpu_count()

fn memory() -> int:
    return runtime.memory()

fn collect_garbage():
    runtime.collect_garbage()

fn stats() -> string:
    stats_json = runtime.stats()
    return stats_json

fn version() -> string:
    version_str = runtime.version()
    return version_str

fn free_string(ptr: string):
    runtime.free(ptr)
